{
  "name": "CatTaxBot",
  "version": "1.0.0",
  "description": "Just a Discord Bot that asks people for the Cat Tax(Pictures of their cats)",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/Bot.ts",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check src/",
    "lint": "eslint src --ext ts --ext js",
    "format": "prettier --write src",
    "test-all": "npm run check-format && npm run lint && npm run check-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DopeLikeJope/CatTaxBot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DopeLikeJope/CatTaxBot/issues"
  },
  "homepage": "https://github.com/DopeLikeJope/CatTaxBot#readme",
  "devDependencies": {
    "@tsconfig/node17": "^1.0.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.6.0"
  }
}
